<h1 class='text-center mb-3 mt-5'>Add new car</h1>
<div class="form-new">
  <%= simple_form_for @car do |f| %>
    <%= f.input :brand %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :address %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.submit %>
  <% end %>
</div>
